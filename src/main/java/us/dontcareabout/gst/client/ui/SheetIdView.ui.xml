<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:c="urn:import:com.sencha.gxt.widget.core.client.container" 
	xmlns:gst="urn:import:us.dontcareabout.gst.client.component">
	
	<ui:with field="gridLD" type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="1" />
	</ui:with>
	
	<ui:with field="h200" type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="200" />
	</ui:with>
	
	<c:VerticalLayoutContainer>
		<gst:SheetIdGrid layoutData="{gridLD}" />
		<gst:SheetIdPanel layoutData="{h200}" />
	</c:VerticalLayoutContainer>
</ui:UiBinder>